#include <iostream>
#include <cstdlib>
#include <time.h>

using namespace std;

int main()
{
   int tablica[10];
   srand(time(NULL));
   cout<<"Tablica: ";

   for (int i=0; i<10; i++)
   {
   	tablica[i]=rand()%200 - 100;
   	cout<<tablica[i]<<"	";
   }


   cout<<"\n"<<"Uporzadkowana tablica: ";
   for(int j=0; j<10;j++)
   {
   for(int i=1;i<10-j;i++)
   {
   	if(tablica[i-1]>tablica[i])
   		swap(tablica[i-1], tablica[i]);
   }
   }

   for(int i=0;i<10;i++)
   {
   	cout<<tablica[i]<<" ";
   }
   
return 0;

}
