#include <iostream>
#include <cstdlib>
#include <time.h>

using namespace std;

int main()
{
   int tablica[10];
	int n;
	int	sum=0;
	int m;
	int	licznik=0;
   srand(time(NULL));
   cout<<"Tablica: ";

   for (int i=0; i<10; i++)
   {
   	tablica[i]=rand()%200 - 100;
   	cout<<tablica[i]<<"	";
   }

   	cout<<"\n"<<"Podaj liczbe: ";
   cin>>m;
   for (int i=0; i<10;i++)
   {
   if(tablica[i]==m)
   	{
   		licznik++;
   	}
   }
   cout<<"Liczba wystapienia podanej liczby: "<<licznik<<endl;
   
   
   return 0;
   
}
